{% if items|length > 0 %}
<ul class="breadcrumb">
    <li><a href="{{ path('homepage') }}"><img src="{{ asset('bundles/worklelayout/images/home.png') }}" /></a> <span class="divider">/</span></li>
    {% for item in items %}

    {% if loop.last %}
    <li class="active"><a href="{{ item.route }}">{{ item.name }}</a></li>
    {% else %}
    <li><a href="{{ item.route }}">{{ item.name }}</a> <span class="divider">/</span></li>
    {% endif %}

    {% endfor %}
</ul>
{% endif %}
